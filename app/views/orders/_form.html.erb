<div class="border p-3 mb-3">
    <div class="d-flex">
        <h5>Assinatura <%= f.object.id unless f.object.new_record? %></h5>
        <%= f.hidden_field :_destroy %>
        <%= link_to '&times;'.html_safe, '#', class: 'btn btn-outline-danger ml-auto remove_order' %>
    </div>
    <%= f.hidden_field :id %>
    <%= f.input :device %>
    <%= f.input :imei %>
    <%= f.input :value %>
    <%= f.input :installments %>
</div>